using System;
class Program
{
    static void Main(string[] args)
    {
        Console.WriteLine("Enter your age: ");
        int age = Convert.ToInt32(Console.ReadLine());
        if (age > 0 && age < 18){
            Console.WriteLine("Minor.");
        }
        else if(age >= 18 && age <= 35){
            Console.WriteLine("Young.");
        }
        else{
            Console.WriteLine("Aged.");
        }
    }
}
